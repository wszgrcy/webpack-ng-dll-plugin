    "@angular/platform-browser": "^14.1.3",
    "@angular/platform-browser-dynamic": "^14.1.3",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/fs-extra": "^9.0.11",
    "@types/jasmine": "^4.3.0",
    "@types/loader-runner": "^2.2.3",
    "cpx": "^1.5.0",
    "cyia-ngx-devkit": "^0.0.3",
    "husky": "^6.0.0",
    "jasmine": "^4.3.0",
    "loader-runner": "^4.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.10",
    "typescript": "~4.7.4",
