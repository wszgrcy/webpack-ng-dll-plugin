{
  "name": "webpack-ng-dll-plugin",
  "version": "2.1.11",
  "description": "ng远程加载插件库",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "pretest": "yarn build:template:dev",
    "test": "jest",
    "build": "rimraf ./dist&& tsc",
    "build:template:dev": "tsc --build tsconfig.template-dev.json",
    "postbuild": "tsc --build tsconfig.template.json",
    "deploy": "typedoc --out docs src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wszgrcy/webpack-ng-dll-plugin.git"
  },
  "author": "wszgrcy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wszgrcy/webpack-ng-dll-plugin/issues"
  },
  "homepage": "https://github.com/wszgrcy/webpack-ng-dll-plugin#readme",
  "peerDependencies": {
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "cyia-code-util": "^1.0.6",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1102.10",
    "@angular-devkit/build-angular": "^0.1102.10",
    "@angular-devkit/core": "^11.2.10",
    "@angular/common": "^11.2.11",
    "@angular/compiler-cli": "^11.2.11",
    "@angular/core": "^11.2.11",
    "@angular/localize": "^11.2.11",
    "@angular/platform-browser": "^11.2.11",
    "@angular/platform-browser-dynamic": "^11.2.11",
    "@angular/platform-server": "^11.2.11",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.22",
    "@types/loader-runner": "^2.2.3",
    "@types/webpack": "^4.41.22",
    "cpx": "^1.5.0",
    "jest": "^26.6.3",
    "loader-runner": "^4.2.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "ts-jest": "^26.5.4",
    "typedoc": "^0.20.36",
    "typescript": "4.1.5",
    "webpack": "^4.44.2",
    "zone.js": "^0.11.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.template.{js,ts}"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
